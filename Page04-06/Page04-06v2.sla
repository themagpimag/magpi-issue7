            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="import pyfirmata"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="from Tkinter import *"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="     "/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="# Create a new board object,"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="# specifying serial port;"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="# could be /dev/ttyUSB0 for older"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="# Arduinos"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="board = pyfirmata.Arduino('/dev/ttyACM0')"/>
            <para LINESP="11"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="# start an iterator thread so"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="# serial buffer doesn't overflow"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="iter8 = pyfirmata.util.Iterator(board)"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="iter8.start()"/>
            <para LINESP="11"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="# set up pins"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="# A0 Input      (LM35)"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="pin0 = board.get_pin('a:0:i')"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="# D3 PWM Output (LED)"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="pin3 = board.get_pin('d:3:p')             "/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="# IMPORTANT! discard first reads"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="# until A0 gets something valid"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="while pin0.read() is None:"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="    pass"/>
            <para LINESP="11"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="def get_temp():"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="    # LM35 reading in deg C to label"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="    label_text = &quot;Temp: %6.1f C&quot; % ("/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="        pin0.read() * 5 * 100)"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="    label.config(text = label_text)"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="    # reschedule after half second"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Bold" FONTSIZE="9" CH="    "/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="root.after(500, get_temp) "/>
            <ITEXT FONT="Courier 10 Pitch Bold" FONTSIZE="9" CH="          "/>
            <para LINESP="11"/>
            <PageItemAttributes/>
        </PAGEOBJECT>

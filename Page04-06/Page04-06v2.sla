            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="def set_brightness(x):"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="    # set LED"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="    # Scale widget returns 0 .. 100"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="    # pyfirmata expects 0 .. 1.0"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="    pin3.write(float(x) / 100.0)"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="    "/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="def cleanup():"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="    # clean up on exit"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="    # and turn LED back off"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="    pin3.write(0)                       "/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="    board.exit()"/>
            <para LINESP="11"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="# set up GUI"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="root = Tk()"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="# ensure cleanup() is called on exit"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="root.wm_protocol(&quot;WM_DELETE_WINDOW&quot;,cleanup)"/>
            <para LINESP="11"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="# draw a big slider for LED brightness"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="scale = Scale(root,"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="              command = set_brightness,"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="              orient = HORIZONTAL,"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="              length = 400,"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="              label = 'Brightness')      "/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="scale.pack(anchor = CENTER)"/>
            <para LINESP="11"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="# place label up against scale widget"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="label = Label(root)"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="label.pack(anchor = 'nw')                 "/>
            <para LINESP="11"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="# start temperature read loop"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="root.after(500, get_temp)"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="# run Tk event loop"/>
            <para LINESP="11"/>
            <ITEXT FONT="Courier 10 Pitch Regular" FONTSIZE="9" CH="root.mainloop()"/>
            <para LINESP="11"/>
